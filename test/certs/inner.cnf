[ req ]
default_bits        = 2048
prompt              = no
default_md          = sha256
req_extensions      = req_ext
distinguished_name  = dn

[ dn ]
CN                  = inner.com

[ req_ext ]
subjectAltName      = @alt_names

[ alt_names ]
DNS.1               = inner.com
DNS.2               = *.inner.com
